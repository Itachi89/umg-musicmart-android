<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.preludesys.umg.musicmart" >
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/us2"
        android:label="@string/app_name"

        android:theme="@style/Theme.AppCompat"
        android:name="com.preludesys.umg.musicmart.MusicMartApplication">
        <activity
            android:name="com.preludesys.umg.musicmart.userinterface.MainActivity"
            android:label="@string/app_name" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.preludesys.umg.musicmart.userinterface.ListScreen" />
    </application>


</manifest>
